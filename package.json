  "license": "ISC",
  "dependencies": {
    "app-root-path": "^1.0.0",
    "body-parser": "^1.13.2",
    "bunyan": "^1.4.0",
    "express": "^4.13.1",
    "express-bunyan-logger": "^1.1.1"
  }
}
