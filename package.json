{
  "name": "ensemblejs-event-capture",
  "version": "1.0.0",
  "description": "Captures events from other ensemblejs projects",
  "main": "src/server.js",
  "scripts": {
    "start": "mkdir -p events/ && node src/server.js | bunyan",
    "sync": "mkdir -p sync/ && mv events/* sync/ || true && aws s3 mv sync s3://ensemblejs-events/in/ --recursive"
  },
  "keywords": [
    "auspex",
    "capture",
    "ensemblejs",
    "event",
    "game",
    "metrics"
  ],
  "author": "Ryan Boucher",
  "license": "ISC",
  "dependencies": {
    "app-root-path": "^1.0.0",
    "body-parser": "^1.13.2",
    "bunyan": "^1.4.0",
    "express": "^4.13.1",
    "express-bunyan-logger": "^1.1.1",
    "git-repo-info": "^1.1.2",
    "mkdirp": "^0.5.1",
    "moment": "^2.11.1",
    "node-uuid": "^1.4.7",
    "present": "0.0.6"
  }
}
